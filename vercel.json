{
  "version": 2,
  "buildCommand": "bash build_files.sh",
  "builds": [
    {
      "src": "jossie_fancies/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/staticfiles/images/favicon.jpg"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "jossie_fancies/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "jossie_fancies.settings"
  },
  "functions": {
    "jossie_fancies/wsgi.py": {
      "runtime": "python3.9"
    }
  }
}