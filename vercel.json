{
  "version": 2,
  "builds": [
    {
      "src": "jossie_fancies/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "jossie_fancies/wsgi.py"
    }
  ],
  "installCommand": "pip install -r requirements.txt && npm install",
  "buildCommand": "npm run build-css-prod && python manage.py collectstatic --noinput"
}